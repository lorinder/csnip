set(@PROJECT_NAME@_VERSION @PROJECT_VERSION@)

@PACKAGE_INIT@

set_and_check(@PROJECT_NAME@_INCLUDEDIR "@PACKAGE_INSTALL_INCLUDEDIR@")
set_and_check(@PROJECT_NAME@_LIBDIR "@PACKAGE_INSTALL_LIBDIR@")

add_library(@PROJECT_NAME@::@PROJECT_NAME@ INTERFACE IMPORTED)
set_target_properties(@PROJECT_NAME@::@PROJECT_NAME@ PROPERTIES
	INTERFACE_INCLUDE_DIRECTORIES "${@PROJECT_NAME@_INCLUDEDIR}"
	INTERFACE_LINK_DIRECTORIES "${@PROJECT_NAME@_LIBDIR}"
	INTERFACE_LINK_LIBRARIES "@PROJECT_NAME@"
)


check_required_components(@PROJECT_NAME@)
